#: src/django_error_views/templates/django_error_views/400.html:3
msgid "Bad request"
msgstr "Nieprawidłowe zapytanie"

#: src/django_error_views/templates/django_error_views/400.html:5
msgid "You have used a bad URL address"
msgstr "Użyto niewłaściwego adresu URL"

#: src/django_error_views/templates/django_error_views/403.html:4
#: src/django_error_views/templates/django_error_views/403.html:7
msgid "Forbidden"
msgstr "Zabroniony"

#: src/django_error_views/templates/django_error_views/404.html:4
msgid "Page not found"
msgstr "Strona nie odnaleziona"

#: src/django_error_views/templates/django_error_views/404.html:6
msgid "We're sorry we don't recognize this URL"
msgstr "Przepraszamy nie rozpoznajemy tego adresu URL"

#: src/django_error_views/templates/django_error_views/500.html:3
msgid "Server error"
msgstr "Błąd serwera"

#: src/django_error_views/templates/django_error_views/500.html:5
msgid "Oops! An error occurred"
msgstr "Ups! Wystąpił błąd"

#: src/django_error_views/templates/django_error_views/500.html:13
msgid "We're sorry for the inconvenience. We should get notified of the error and we'll correct it as soon as possible"
msgstr "Przepraszamy za utrudnienia. Powinniśmy zostać powiadomieni o tym problemie i spróbujemy go wkrótce rozwiązać"

#: src/django_error_views/templates/django_error_views/error_actions.html:3
msgid "Go to homepage"
msgstr "Idź do strony domowej"

#: src/django_error_views/templates/django_error_views/error_actions.html:4
msgid "Please fix the error."
msgstr "Proszę o rozwiązanie problemu"

#: src/django_error_views/templates/django_error_views/error_actions.html:5
msgid "Ask for help"
msgstr "Poproś o pomoc"

